{
  "name": "liontechir_cypress",
  "version": "1.0.1",
  "description": "Automacao de testes UI utilizando o Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:mobile": "cypress open --config viewportWidth=768 viewportHeight=812",
    "cy:run": "cypress run",
    "cy:run:mobile": "cypress run --config viewportWidth=768 viewportHeight=812",
    "cy:chrome": "cypress run --browser chrome",
    "cy:firefox": "cypress run --browser firefox",
    "cy:edge": "cypress run --browser edge",
    "cy:balanco": "cypress run --spec 'cypress/e2e/balanco.cy.js' ",
    "cy:carteiraPosition": "cypress run --spec 'cypress/e2e/carteiraPosition.cy.js' ",
    "cy:darf": "cypress run --spec 'cypress/e2e/darf.cy.js' ",
    "cy:forgotPassword": "cypress run --spec 'cypress/e2e/forgotPassword.cy.js' ",
    "cy:home": "cypress run --spec 'cypress/e2e/home.cy.js' ",
    "cy:login": "cypress run --spec 'cypress/e2e/login.cy.js' ",
    "cy:signup": "cypress run --spec 'cypress/e2e/signup.cy.js' ",
    "cy:transaction": "cypress run --spec 'cypress/e2e/transaction.cy.js' ",
    "allure:clear": "rm -r allure-results/ allure-report || true",
    "allure:generate": "allure generate allure-results",
    "allure:open": "allure open allure-report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kbmbarreto/liontechir_cypress.git"
  },
  "author": "Kleber Barreto de Macedo",
  "contributor": [
    "Vini Santos"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kbmbarreto/liontechir_cypress/issues"
  },
  "homepage": "https://github.com/kbmbarreto/liontechir_cypress#readme",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.28.5",
    "cypress": "^12.5.1",
    "http-server": "^14.1.1",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2"
  },
  "dependencies": {
    "cypress-plugin-steps": "^1.1.0",
    "cypress-plugin-xhr-toggle": "^1.0.0",
    "run-all": "1.0.1"
  }
}
